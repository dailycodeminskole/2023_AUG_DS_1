# strip will remove only the leading and trailing spaces
#  not the one in between

place = "                Pune"
print(place)
print(place.strip())

s = "            Pune is                   Good!!!"
print(s.strip())

# boolean o/p
#  for validation abd cheking the input

password = 'pass 123'
print(password.isalnum()) #only alphabets and numbers

mobilenum = "a123456789"
print(mobilenum.isdigit())


name = "Prakash007"
print(name.isalpha()) #all the char should be alphabets only

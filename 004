# HW
a = 'One'
print(type(a))

b = " "
print(type(b)) #str

c = "101"
print(type(c))

p = 100
print(type(p))


p = 101.4
print(type(p)) #float

d = '3.14'
print(type(d))


# STRINGS


skill = "PYTHON"
print(skill)

type(skill)
print(type(skill))



# indexing	0	1	2	3	4	5
# STRING 	  P	Y	T	H	O	N
skill = "PYTHON"

print(skill[0])
print(skill[1])
print(skill[2])
print(skill[5])
print(skill[4])

#        0123456789...
email = "MINSKOLE@GMAIL.COM"

print(email[8]) # @
print(email[0]) # M
print(email[5]) # O



